#include<stdio.h> 
struct init
{
   int a, b, c;
}; 
int main()
{
   // using designated initialization
   struct init i1 = {.b = 1, .a = 4, .c = 47};
   struct init i2 = {.b = 13};
   printf("TechVidvan Tutorial: Designated initialization!\n\n");
   printf ("Values: a = %d, b = %d, c = %d\n", i1.a,i1.b,i1.c);
   printf ("Values: b = %d", i2.b);
   return 0;
}
